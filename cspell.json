{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en,en-US",
  "allowCompoundWords": true,
  "words": [
    "axios",
    "commitlint",
    "dotenv",
    "ependencies",
    "esbuild",
    "kamil",
    "lefthook",
    "nestjs",
    "nocheck",
    "pinia",
    "plusplus",
    "prettier",
    "prisma",
    "rollup",
    "sinopec",
    "taze",
    "turbo",
    "typecheck",
    "unbuild",
    "unplugin",
    "unref",
    "upsert",
    "vben",
    "vetur",
    "vite",
    "vitejs",
    "vitest",
    "vueuse"
  ],
  "ignorePaths": [
    "**/*.log",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/__tests__/**",
    "**/dist/**",
    "**/node_modules/**",
    "pnpm-lock.yaml"
  ]
}
