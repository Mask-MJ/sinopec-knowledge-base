    "@nestjs/core": "catalog:",
    "@nestjs/event-emitter": "catalog:",
    "@nestjs/jwt": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@nestjs/schedule": "catalog:",
    "@nestjs/swagger": "catalog:",
    "@prisma/adapter-pg": "catalog:",
    "@prisma/client": "catalog:",
    "axios": "catalog:",
    "bcrypt": "catalog:",
    "cache-manager": "catalog:",
    "class-transformer": "catalog:",
    "class-validator": "catalog:",
    "dayjs": "catalog:",
    "helmet": "catalog:",
    "ioredis": "catalog:",
    "ip2region": "catalog:",
    "lodash-es": "catalog:",
    "minio": "catalog:",
    "nest-winston": "catalog:",
    "nestjs-prisma": "catalog:",
    "p-limit": "catalog:",
    "pg": "catalog:",
    "prisma-extension-pagination": "catalog:",
    "reflect-metadata": "catalog:",
    "request-ip": "catalog:",
    "rxjs": "catalog:",
    "systeminformation": "catalog:",
    "winston": "catalog:",
    "winston-daily-rotate-file": "catalog:",
    "xlsx": "catalog:"
